{
    "compilerOptions": {
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "noUnusedParameters": false,
        "noUnusedLocals": false,
        "lib": [
            "ES2018"
        ],
        "module": "commonjs",
        "target": "es5",
        "noImplicitAny": true,
        "moduleResolution": "node",
        "sourceMap": true,
        "outDir": "dist"
    }
    
}